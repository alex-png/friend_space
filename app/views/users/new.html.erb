<div id="home">

<font color="white"> 
<center><h1>sign up for friendspace!</h1>
<h2>just fill out this form</h2></center>

<center>
<%= form_for :user do |f|%>
  user name:<p> <%=f.text_field :user_name%>
  <p>
  password: <p> <%=f.password_field :password%>
  <p>
  name: <p> <%=f.text_field :name%>
  <p>
  age: <p><%=f.number_field :age%>
  <p>
  quick bio: <p> <%=f.text_area :bio%>
  <p>
  profile picture (image url):<p><%=f.text_field :image%>
  <p>
  song (youtube id; ex: "9jfSUq15xd0"):<p><%=f.text_field :song%>
  <p>
  <%= f.submit "sign up" %>
<%end%>
</font></center>
<%if flash[:message]%>
<font color="red"><%=flash[:message].to_sentence%></font>
<%end%>

<br>
